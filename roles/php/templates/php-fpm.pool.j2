[www]
user = {{ main_user }}
group = {{ main_user }}
listen = /var/run/php/php{{ php.version }}-fpm.sock
pm = dynamic
pm.max_children = 5
pm.start_servers = 2
pm.min_spare_servers = 1
pm.max_spare_servers = 3
listen.owner = nginx
listen.group = nginx
listen.mode = 0660
security.limit_extensions=.php